{
  "name": "api-testing-node-fetch",
  "version": "1.0.0",
  "description": "automação de testes de api com node-fetch",
  "main": "index.js",
  "scripts": {
    "test": "mocha --experimental-modules --reporter mochawesome --reporter-options reportDir=./mochawesome-report ./tests/*.mjs",
    "clean": "rm -rf mochawesome-report",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:cache": "eslint . --cache",
    "format": "prettier --write ."
  },
  "keywords": [
    "node-fetch",
    "api testing",
    "test automation"
  ],
  "author": "gustavo cardoso",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.3.7",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "esm": "^3.2.25",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "node-fetch": "^3.3.2",
    "prettier": "^3.0.0"
  }
}
